{
  "name": "ssh2-fs",
  "description": "Transparent use of the `fs` module locally or over SSH",
  "version": "1.0.1",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [],
  "dependencies": {},
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "mocha": "~7.1.1",
    "should": "~13.2.3",
    "ssh2-exec": "~0.5.3",
    "ssh2-they": "~2.1.0"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "homepage": "https://github.com/adaltas/node-ssh2-fs",
  "keywords": [
    "child process",
    "fs",
    "ssh",
    "ssh2"
  ],
  "license": "BSD-3-Clause",
  "main": "./lib/index",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register"
    ],
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-ssh2-fs.git"
  },
  "scripts": {
    "preversion": "rm -rf lib && npm test && grep '## Trunk' CHANGELOG.md",
    "version": "version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "coffee": "coffee -b -o lib src",
    "pretest": "coffee -b -o lib src",
    "test": "mocha test/*.coffee"
  }
}
