{
  "name": "ssh2-fs",
  "description": "Transparent use of the `fs` module locally or over SSH",
  "version": "1.1.0",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [],
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.3",
    "coffeescript": "^2.6.1",
    "husky": "^7.0.4",
    "mocha": "~9.2.2",
    "mocha-they": "^0.1.3",
    "should": "~13.2.3",
    "ssh2-connect": "^3.4.0",
    "ssh2-exec": "~0.7.3",
    "standard-version": "^9.3.2"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "homepage": "https://github.com/adaltas/node-ssh2-fs",
  "keywords": [
    "child process",
    "fs",
    "ssh",
    "ssh2"
  ],
  "license": "BSD-3-Clause",
  "main": "./lib/index",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register"
    ],
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "optionalDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-ssh2-fs.git"
  },
  "scripts": {
    "build": "coffee -b -o lib src",
    "prepare": "husky install",
    "prepublishOnly": "git push --follow-tags origin master",
    "pretest": "npm run build",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "mocha 'test/**/*.coffee'"
  }
}
